
@model IEnumerable<Microsoft.AspNetCore.Identity.IdentityRole>
@{
    ViewData ["Title"] = "Role";
}

<h1>List of Roles</h1>
<a asp-action="Create" class="btn btn-outline-deep-orange btn-sm">Create</a>
<table class="table table-striped table-bordered">
    <thead class="table-danger">
        <tr>
            @*<td>Id</td>*@
            <td>Name</td>
        </tr>
    </thead>
    <tbody>
        @foreach ( var role in Model )
        {
            <tr>
                @*<td>@role.Id</td>*@
                <td>@role.Name</td>
            </tr>
        }
    </tbody>
</table>