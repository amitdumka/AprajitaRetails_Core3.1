@model IEnumerable<AprajitaRetails.Models.EDC>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.TID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EDCName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AccountNumberId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StartDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EndDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IsWorking)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Remark)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Store)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EDCName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AccountNumberId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StartDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EndDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IsWorking)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Remark)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Store.StoreId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.EDCId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.EDCId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.EDCId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
