@model IEnumerable<AprajitaRetails.Areas.Uploader.Models.ImportSaleRegister>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
