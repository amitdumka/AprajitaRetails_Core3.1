                <a asp-action="Details" asp-route-id="@item.ImportSaleRegisterId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ImportSaleRegisterId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
