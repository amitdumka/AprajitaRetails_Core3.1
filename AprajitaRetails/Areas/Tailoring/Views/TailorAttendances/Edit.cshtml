@model AprajitaRetails.Models.TailorAttendance

@{
    ViewData["Title"] = "Edit";
}



<h4>TailorAttendance</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="TailorAttendanceId" />
            <div class="form-group">
                <label asp-for="TailoringEmployeeId" class="control-label"></label>
                <select asp-for="TailoringEmployeeId" class="form-control" asp-items="ViewBag.TailoringEmployeeId"></select>
                <span asp-validation-for="TailoringEmployeeId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="AttDate" class="control-label"></label>
                <input asp-for="AttDate" class="form-control" />
                <span asp-validation-for="AttDate" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="EntryTime" class="control-label"></label>
                <input asp-for="EntryTime" class="form-control" />
                <span asp-validation-for="EntryTime" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Status" class="control-label"></label>
                <select asp-for="Status" class="form-control"></select>
                <span asp-validation-for="Status" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Remarks" class="control-label"></label>
                <input asp-for="Remarks" class="form-control" />
                <span asp-validation-for="Remarks" class="text-danger"></span>
            </div>
            <div class="form-group">
               <input type="submit" value="Save" class="btn btn-outline-amber" />
            </div>
        </form>
    </div>
</div>

<div>
  
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
